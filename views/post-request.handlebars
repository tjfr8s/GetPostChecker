<h1>POST Request Received</h1>


<table>
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Value
            </th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>
                {{this.name}}
            </td>
            <td>
                {{this.value}}
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
